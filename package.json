        "@types/sinon": "^9.0.0",
        "@types/supertest": "^2.0.8",
        "@types/twitter": "^1.7.0",
        "@types/uuid": "^7.0.2",
        "@types/web3": "^1.2.2",
        "@types/winston": "^2.4.4",
        "artillery": "^1.6.1",
        "chai": "^4.2.0",
        "cross-env": "^7.0.3",
