        "@types/bs58": "^4.0.1",
        "@types/chai": "^4.2.11",
        "@types/cls-hooked": "^4.3.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/eth-sig-util": "^2.1.0",
