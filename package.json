{
  "name": "raiinmaker-server",
  "version": "0.0.7",
  "main": "index.js",
  "repository": "git@github.com:dragonchain-inc/raiinmaker-server.git",
  "author": "Alex Benedetto <alex061994@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "TS_NODE_FILES=true NODE_ENV=development ts-node src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist/",
    "postbuild": "cp src/graphql/*.graphql dist/graphql/",
    "lint": "eslint --ext .ts --format unix src/ && prettier --check 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha -r ts-node/register 'src/**/*.spec.ts' --exit",
    "test:built": "NODE_ENV=test mocha 'dist/**/*.spec.js' --exit",
    "update:cicd": "aws cloudformation deploy --template-file cicd.cft.yml --stack-name creatorworks-cicd --capabilities CAPABILITY_NAMED_IAM",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm:cli migration:run",
    "migration:revert": "yarn typeorm:cli migration:revert",
    "migration:generate": "yarn typeorm:cli migration:generate"
  },
  "dependencies": {
    "@dragonchain-dev/dragonfactor-auth": "/Users/alexbenedetto/Documents/git/dragonfactor-auth",
    "ajv": "^6.12.2",
    "aws-sdk": "^2.632.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^1.4.0",
    "cors": "^2.8.5",
    "dragonchain-sdk": "^4.3.3",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "firebase-admin": "^8.10.0",
    "graphql": "^14.6.0",
    "graphql-rate-limit": "^2.0.1",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.0",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "redis": "^3.0.2",
    "twitter": "^1.7.1",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.19.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.2",
    "@types/redis": "^2.8.18",
    "@types/twitter": "^1.7.0",
    "@types/uuid": "^7.0.2",
    "@types/winston": "^2.4.4",
    "typescript": "^3.8.3"
  }
}
