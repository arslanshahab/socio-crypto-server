        "cls-hooked": "^4.2.2",
        "cls-rtracer": "^1.4.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cryptoicons-cdn": "^0.1.30",
        "date-fns": "^2.23.0",
        "dragonchain-sdk": "^4.3.3",
        "eth-sig-util": "^2.5.3",
        "ethereumjs-util": "^7.0.5",
        "express": "^4.17.1",
        "firebase-admin": "^9.2.0",
        "forex-rates": "^1.0.3",
        "form-data": "^4.0.0",
        "graphql": "^14.6.0",
        "graphql-rate-limit": "^2.0.1",
        "graphql-type-json": "^0.3.1",
        "js-sha3": "^0.8.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "mocha": "^7.1.0",
        "node-fetch": "^2.6.0",
        "paypal-rest-sdk": "^1.8.1",
        "pg": "^8.0.3",
        "prisma": "^3.11.0",
        "redis": "^3.0.2",
